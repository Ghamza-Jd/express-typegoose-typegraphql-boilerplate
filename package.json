{
  "name": "express-mongo-typegraphql-boilerplate",
  "version": "0.1.0",
  "description": "boilerplate code for working with express, mongodb, graphql and typescript",
  "main": "src/app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "tsnd --respawn --cls --notify=false src/app.ts",
    "build": "tsc -p .",
    "lint": "yarn run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "yarn run lint:eslint src/",
    "lint:fix": "yarn run lint:js --fix"
  },
  "author": "Hamza Jadid",
  "repository": {
    "type": "git",
    "url": "https://github.com/HamzaJadid/express-typegoose-typegraphql-boilerplate.git"
  },
  "license": "MIT",
  "dependencies": {
    "@hasezoey/typegoose": "^5.9.2",
    "@types/graphql": "^14.5.0",
    "apollo-server-express": "^2.12.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "14.1.1",
    "mongoose": "^5.9.10",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.14",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "ts-node": "^8.9.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  }
}
